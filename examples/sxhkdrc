super + Return
    urxvt

super + {_,ctrl + ,alt + }{Up,Down,Left,Right}
    custardctl window -{move,expand,contract} {up,down,left,right}

super + {_,ctrl +,alt +}{1-9,0}
    custardctl workspace -{focus,attach,detach} {1-9,10}

super + shift + {1-9,0}
    custardctl window -relocate {1-9,10}

super + w
    custardctl window -close

super + Escape
    custardctl -stop

super + plus
    custardctl -focus next

super + minus
    custardctl -focus prev
